{
    "manifest_version": 3,
    "name": "Dependify Chrome Extension",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png"
    },
    "icons": {
      "48": "icons/icon.png"
    }
  }
  